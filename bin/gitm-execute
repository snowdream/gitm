#!/usr/bin/env node

exports = module.exports = function (command) {
    var cp = require('child_process');
    var execSync = cp.execSync;

    if (!command) {
        console.warn("command should not be null or empty.")
        return;
    }

    console.info("$ " + command);


    var child = execSync(command);
    console.log(child);
};
